&lt;p&gt;function deldir($dir){&lt;br /&gt;
&amp;nbsp; $current_dir = opendir($dir);&lt;br /&gt;
&amp;nbsp; while($entryname = readdir($current_dir)){&lt;br /&gt;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; if(is_dir(&amp;quot;$dir/$entryname&amp;quot;) and ($entryname != &amp;quot;.&amp;quot; and $entryname!=&amp;quot;..&amp;quot;)){&lt;br /&gt;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; $this-&amp;gt;deldir(&amp;quot;${dir}/${entryname}&amp;quot;);&lt;br /&gt;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; }elseif($entryname != &amp;quot;.&amp;quot; and $entryname!=&amp;quot;..&amp;quot;){&lt;br /&gt;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; unlink(&amp;quot;${dir}/${entryname}&amp;quot;);&lt;br /&gt;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; }&lt;br /&gt;
&amp;nbsp; }&lt;br /&gt;
&amp;nbsp; closedir($current_dir);&lt;br /&gt;
&amp;nbsp; rmdir(${dir});&lt;br /&gt;
