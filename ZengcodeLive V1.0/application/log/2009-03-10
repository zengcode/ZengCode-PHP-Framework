&amp;lt;form method=&amp;quot;post&amp;quot;&amp;gt;&lt;br /&gt;
&amp;lt;textarea name=&amp;quot;contents&amp;quot; cols=&amp;quot;100&amp;quot; rows=&amp;quot;10&amp;quot;&amp;gt;&amp;lt;?=$str?&amp;gt;&amp;lt;/textarea&amp;gt;&lt;br /&gt;
&amp;lt;br&amp;gt;&amp;lt;input type=&amp;quot;submit&amp;quot; value=&amp;quot;Overwrite This File&amp;quot; onclick=&amp;quot;return confirm(\'Do you want to overwrite this file\');&amp;quot;&amp;gt;&lt;br /&gt;
&amp;lt;/form&amp;gt;&lt;br /&gt;
